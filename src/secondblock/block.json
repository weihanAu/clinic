{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "hudsonblocks/secondblock",
	"version": "0.1.0",
	"title": "secondblock",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"attributes": {
		"postsPerPage": {
            "type": "number",
            "default": 3
        },
        "currentPage": {
            "type": "number",
            "default": 1
        },
        "category": {
            "type": "number",
            "default": 0
        },
				"src": {
						"type": "string",
						"selector": "img",
						"attribute": "src",
						"source": "attribute"
	    	}
	},
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "hudsonblocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
